<!-- users.index.html.erb -->


<% if user_logged_in? %>

  <h1>Welcome <%= current_user.name %></h1>
    <%= link_to "Home", root_path %>


    <ul>
    <% @users.each do |user| %>
        <li><br><strong>Name:</strong> <%= user.name%>, <strong>Email:</strong> <%= user.email %>, <strong>Phone:</strong> <%= user.phone_number %></li>
    <% end %>
    </ul>
<% else %>
  <h1>You are not logged in<h1>
<% end %>
